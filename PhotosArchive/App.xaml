<Application x:Class="PhotosArchive.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PhotosArchive"
             xmlns:vm="clr-namespace:PhotosArchive.Locator"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelsLocator x:Key="Locator"/>
            <BooleanToVisibilityConverter x:Key="visible"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionnaire/BoutonStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
